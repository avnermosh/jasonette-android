"$jason": {
    "head": {
        "actions": {
            "$load": {
                "type": "$agent.request",
                "options": {
                    "id": "$webcontainer"
                }
            }
        }
    },
    "body": {
        "background": {
            "type": "html",
            "style": {
                "progress": "rgba(255,255,255,0.0)"
            },
            "url": "file://root/html/raw/index.html",
            "action": {
                "type": "$default"
            }
        }
    }
}
